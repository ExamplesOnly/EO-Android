<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ToggleIconButton">
        <attr name="icon_type" format="string" />
        <attr name="icon_size" format="dimension|reference"/>
        <attr name="toggle_enable_drawable" format="reference" />
        <attr name="toggle_disable_drawable" format="reference" />
